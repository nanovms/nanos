OUTPUT_FORMAT("elf64-x86-64")

ENTRY(_start)

SECTIONS
{
  . = 0x7f000000;
  START = .;
  .start ALIGN(4096): { *(.start)}
  /* the default linker aligns the file and text without throwing
     away a page..but for today...*/
  .text : ALIGN(4096) { *(.text)}
  .rodata : { *(.rodata)}
  .data : { *(.data) }
   PROVIDE(bss_start = .);
  .bss  ALIGN(32): { *(.bss) }
   PROVIDE(bss_end = .);
}
